package cn.onedirection.pojo;

import java.util.Date;

/**
 * 比赛/活动
 */
public class Activity {
	private int act_ID;    				// 比赛id
	private String act_name;			// 名称
	private Date act_starttime; 		// 开始时间 
	private Date act_signtime; 			// 报名截止时间 
	private String act_station;			// 地点
	private int act_pepnum;				// 参赛总人数
	private String act_content;			// 比赛简介内容
	private int act_status;				// 1.竞赛 2.活动
	private String act_remak;			// 备注
	private Date act_createtime;		// 创建时间
	public int getAct_ID() {
		return act_ID;
	}
	public void setAct_ID(int act_ID) {
		this.act_ID = act_ID;
	}
	public String getAct_name() {
		return act_name;
	}
	public void setAct_name(String act_name) {
		this.act_name = act_name;
	}
	public Date getAct_starttime() {
		return act_starttime;
	}
	public void setAct_starttime(Date act_starttime) {
		this.act_starttime = act_starttime;
	}
	public Date getAct_signtime() {
		return act_signtime;
	}
	public void setAct_signtime(Date act_signtime) {
		this.act_signtime = act_signtime;
	}
	public String getAct_station() {
		return act_station;
	}
	public void setAct_station(String act_station) {
		this.act_station = act_station;
	}
	public int getAct_pepnum() {
		return act_pepnum;
	}
	public void setAct_pepnum(int act_pepnum) {
		this.act_pepnum = act_pepnum;
	}
	public String getAct_content() {
		return act_content;
	}
	public void setAct_content(String act_content) {
		this.act_content = act_content;
	}
	public int getAct_status() {
		return act_status;
	}
	public void setAct_status(int act_status) {
		this.act_status = act_status;
	}
	public String getAct_remak() {
		return act_remak;
	}
	public void setAct_remak(String act_remak) {
		this.act_remak = act_remak;
	}
	public Date getAct_createtime() {
		return act_createtime;
	}
	public void setAct_createtime(Date act_createtime) {
		this.act_createtime = act_createtime;
	}
	@Override
	public String toString() {
		return "Activity [act_ID=" + act_ID + ", act_name=" + act_name + ", act_starttime=" + act_starttime
				+ ", act_signtime=" + act_signtime + ", act_station=" + act_station + ", act_pepnum=" + act_pepnum
				+ ", act_content=" + act_content + ", act_status=" + act_status + ", act_remak=" + act_remak
				+ ", act_createtime=" + act_createtime + "]";
	}

}
